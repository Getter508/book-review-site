<h2>Users</h2>

<div class="users">
  <ul>
    <% @users.each do |user| %>
      <div class="user">
        <li>
          <%= user.avatar_url %>
          <%= user.display_name %>
          <%= user.email %>
          <%= link_to 'Delete', admin_user_path(user), method: :delete %>
        </li>
      </div>
    <% end %>
  </ul>
</div>
